[core]
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
        editor=nvim
[user]
        email = joanchenuk@gmail.com
        name = Joan Chen

[init]
        defaultBranch = main

[fetch]
        prune = true

[status]
        short = true
[branch "main"]
        remote = origin
        merge = refs/heads/main

[credential "https://github.com"]
	helper =
	helper = !/home/joanchen/.asdf/installs/github-cli/2.11.3/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper =
	helper = !/home/joanchen/.asdf/installs/github-cli/2.11.3/bin/gh auth git-credential
[alias]
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
